<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dawidd6.hellothere.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/bt9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="2dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt9"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toRightOf="@+id/bt8"
        app:layout_constraintTop_toBottomOf="@+id/bt6"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/bt8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt8"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageButton
        android:id="@+id/bt7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="2dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt7"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintRight_toLeftOf="@+id/bt8"
        app:layout_constraintTop_toBottomOf="@+id/bt4"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/bt6"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginTop="2dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt6"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toLeftOf="@+id/bt9"
        app:layout_constraintRight_toRightOf="@+id/bt9"
        app:layout_constraintTop_toBottomOf="@+id/bt3"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/bt5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt5"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageButton
        android:id="@+id/bt4"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginTop="2dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt4"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toLeftOf="@+id/bt7"
        app:layout_constraintRight_toRightOf="@+id/bt7"
        app:layout_constraintTop_toBottomOf="@+id/bt1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/bt3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt3"
        android:elevation="0dp"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintLeft_toLeftOf="@+id/bt6"
        app:layout_constraintRight_toRightOf="@+id/bt6"
        app:layout_constraintTop_toTopOf="@+id/bt2"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/bt2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt2"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageButton
        android:id="@+id/bt1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        android:contentDescription="bt1"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintLeft_toLeftOf="@+id/bt4"
        app:layout_constraintRight_toRightOf="@+id/bt4"
        app:layout_constraintTop_toTopOf="@+id/bt2"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="63dp"
        android:layout_height="34dp"
        android:text="init"
        android:textAlignment="center"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="304dp"
        android:layout_height="2dp"
        android:layout_marginEnd="7dp"
        android:layout_marginStart="7dp"
        android:background="@android:color/background_dark"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt5"
        tools:background="@android:color/background_dark"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="304dp"
        android:layout_height="2dp"
        android:layout_marginEnd="6dp"
        android:layout_marginStart="6dp"
        android:background="@android:color/background_dark"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt2"
        tools:background="@android:color/background_dark"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="2dp"
        android:layout_height="304dp"
        android:layout_marginBottom="4dp"
        android:background="@android:color/background_dark"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/bt7"
        app:layout_constraintLeft_toRightOf="@+id/bt1"
        app:layout_constraintRight_toRightOf="@+id/bt1"
        app:layout_constraintTop_toTopOf="@+id/bt1"
        tools:background="@android:color/background_dark"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="2dp"
        android:layout_height="304dp"
        android:background="@android:color/background_dark"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/bt2"
        app:layout_constraintRight_toRightOf="@+id/bt2"
        app:layout_constraintTop_toTopOf="@+id/bt2"
        app:layout_constraintVertical_bias="1.0"
        tools:background="@android:color/background_dark"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="restartGame"
        android:text="Restart"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="32dp" />

</android.support.constraint.ConstraintLayout>
